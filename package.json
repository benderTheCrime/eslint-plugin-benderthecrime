{
  "name": "eslint-plugin-benderthecrime",
  "version": "2.2.1",
  "description": "shareable eslint plugin",
  "author": "Joe Groseclose <@benderTheCrime>",
  "private": false,
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/benderTheCrime/eslint-plugin-benderthecrime"
  },
  "bugs": {
    "url": "https://github.com/benderTheCrime/eslint-plugin-benderthecrime/issues"
  },
  "engines": {
    "node": ">6",
    "npm": ">=3.10.8",
    "yarn": ">=0.23.2"
  },
  "scripts": {
    "lint:eslint": "./node_modules/.bin/eslint .",
    "lint:fix": "./node_modules/.bin/eslint . --fix",
    "lint": "npm run lint:eslint",
    "test:mocha": "./node_modules/.bin/mocha test --recursive",
    "test": "npm run test:mocha",
    "precommit": "npm run lint:eslint"
  },
  "dependencies": {
    "requireindex": "^1.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.6.0",
    "husky": "^0.13.4",
    "mocha": "^3.2.0"
  }
}
